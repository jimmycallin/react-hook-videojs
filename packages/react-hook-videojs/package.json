{
  "name": "react-hook-videojs",
  "version": "3.0.2",
  "description": "A simple react hook to easily integrate video.js with React",
  "author": "jimmycallin",
  "license": "MIT",
  "repository": "jimmycallin/react-hook-videojs",
  "types": "./dist/index.d.ts",
  "main": "dist/react-hook-videojs.umd.js",
  "module": "dist/react-hook-videojs.es.js",
  "source": "src/index.tsx",
  "engineStrict": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rimraf dist .tmp",
    "build": "vite build && tsc --project tsconfig.types.json",
    "test": "npm run test:vitest",
    "test:vitest": "vitest run",
    "format": "prettier --write .",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run test",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "react": ">=16.8.0 < 19",
    "react-dom": ">=16.8.0 < 19",
    "video.js": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/video.js": "^7.3.45",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "video.js": "^7.20.2",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "dequal": "^2.0.3",
    "lodash.clonedeep": "^4.5.0"
  }
}
