{
  "name": "react-hook-videojs",
  "version": "2.1.0-beta.2",
  "description": "A simple react hook to easily integrate video.js with React",
  "author": "jimmycallin",
  "license": "MIT",
  "repository": "jimmycallin/react-hook-videojs",
  "types": "./dist/index.d.ts",
  "main": "dist/react-hook-videojs.umd.js",
  "module": "dist/react-hook-videojs.es.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "rimraf dist .tmp",
    "build": "vite build && tsc --project tsconfig.types.json",
    "test": "npm run test:typescript && npm run test:lint && npm run test:jest && npm run test:formatting",
    "test:typescript": "tsc --project tsconfig.types.json",
    "test:jest": "jest",
    "test:lint": "eslint ./src",
    "test:formatting": "prettier -c .",
    "format": "prettier --write .",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run test"
  },
  "peerDependencies": {
    "react": "^16.8.0, ^17.0.0",
    "video.js": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-typescript": "^7.18.8",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^28.1.5",
    "@types/video.js": "^7.3.42",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "^28.1.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "video.js": "^7.13.3",
    "vite": "^2.4.4"
  },
  "files": [
    "dist"
  ]
}
