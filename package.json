{
  "name": "react-hook-videojs",
  "version": "2.1.0-beta.1",
  "description": "A simple react hook to easily integrate video.js with React",
  "author": "jimmycallin",
  "license": "MIT",
  "repository": "jimmycallin/react-hook-videojs",
  "main": "dist/react-hook-videojs.umd.js",
  "module": "dist/react-hook-videojs.es.js",
  "source": "src/index.jsx",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "test": "npm run test:lint && npm run test:jest && npm run test:formatting",
    "test:jest": "jest",
    "test:lint": "eslint --ext .js --ext .jsx .",
    "test:formatting": "prettier -c .",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run test"
  },
  "peerDependencies": {
    "react": "^16.8.0, ^17.0.0",
    "video.js": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.0.1",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "video.js": "^7.13.3",
    "vite": "^2.4.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {},
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
